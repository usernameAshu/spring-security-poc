        <mxCell id="KeeJ4AFskf9lS32YRy0w-45" value="8. Returns the UserDetails object" style="text;html=1;strokeColor=none;fillColor=none;align=center;verticalAlign=middle;whiteSpace=wrap;rounded=0;" parent="1" vertex="1">
